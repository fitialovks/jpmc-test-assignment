{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "MultiValidationResponse",
  "type": "object",
  "required": [
    "result"
  ],
  "properties": {
    "result": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "source": {
            "type": "string"
          },
          "isValid": {
            "type": "boolean"
          }
        },
        "required": [
          "source",
          "isValid"
        ],
        "additionalProperties": false
      }
    }
  }
}